  <collaboration id="Collaboration">
    <participant id="pool1" name="Save Energy" processRef="SaveEnerge"></participant>
  </collaboration>
  <process id="SaveEnerge" name="process_pool1" isExecutable="true">
    <laneSet id="laneSet_SaveEnerge">
      <lane id="lane1" name="out door">
        <flowNodeRef>timerstartevent1</flowNodeRef>
        <flowNodeRef>scripttask1</flowNodeRef>
        <flowNodeRef>usertask1</flowNodeRef>
      </lane>
      <lane id="lane2" name="Lab 1">
        <flowNodeRef>scripttask3</flowNodeRef>
        <flowNodeRef>exclusivegateway1</flowNodeRef>
        <flowNodeRef>scripttask4</flowNodeRef>
        <flowNodeRef>scripttask5</flowNodeRef>
        <flowNodeRef>scripttask6</flowNodeRef>
        <flowNodeRef>scripttask7</flowNodeRef>
        <flowNodeRef>scripttask8</flowNodeRef>
        <flowNodeRef>endevent1</flowNodeRef>
        <flowNodeRef>scripttask9</flowNodeRef>
      </lane>
    </laneSet>
    <startEvent id="timerstartevent1" name="Timer start">
      <timerEventDefinition>
        <timeCycle>R/PT30S</timeCycle>
      </timerEventDefinition>
    </startEvent>
    <scriptTask id="scripttask1" name="outdoor temperature" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>com.diplab.db.SensorMapper mapper = sqlSession.getMapper(com.diplab.db.SensorMapper.class);
float TO = mapper.selectAvgByTime("TEMPERATURE", "Device127", 5);
out.format("SaveEnerge outdoor temp = %10f degree\n", TO);
execution.setVariable("TO",TO);
</script>
    </scriptTask>
    <sequenceFlow id="flow1" sourceRef="timerstartevent1" targetRef="scripttask1"></sequenceFlow>
    <scriptTask id="scripttask3" name="indoor temperature" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>com.diplab.db.SensorMapper mapper = sqlSession.getMapper(com.diplab.db.SensorMapper.class);
float TA = mapper.selectAvgByTime("TEMPERATURE", "Device135", 5);
out.format("SaveEnergy room temp = %10f degree\n", TA);
execution.setVariable("TA",TA);
</script>
    </scriptTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway" default="flow26"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="scripttask3" targetRef="exclusivegateway1"></sequenceFlow>
    <scriptTask id="scripttask4" name="set target temp 25" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy set ac temp as 25");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().setACtemperature(25);
java.util.concurrent.TimeUnit.MILLISECONDS.sleep(1000);
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().cooler();</script>
    </scriptTask>
    <sequenceFlow id="flow7" name="outdoor &gt; 32 &amp; indoor &gt;30" sourceRef="exclusivegateway1" targetRef="scripttask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${TO>32 && TA>30}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask5" name="open fans A" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy open fans A");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().on();</script>
    </scriptTask>
    <sequenceFlow id="flow8" sourceRef="scripttask4" targetRef="scripttask5"></sequenceFlow>
    <scriptTask id="scripttask6" name="set target temp 28" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy set ac temp to 28");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().setACtemperature(28);
java.util.concurrent.TimeUnit.MILLISECONDS.sleep(1000);
out.println("SaveEnergy set cooler");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().cooler();</script>
    </scriptTask>
    <sequenceFlow id="flow9" name="outdoor &lt; 28 &amp; indoor &gt; 30" sourceRef="exclusivegateway1" targetRef="scripttask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${TO<28 && TA>30}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask7" name="trun to blast" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy turn to blast");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().blast();</script>
    </scriptTask>
    <sequenceFlow id="flow10" name="outdoor &lt; 28 &amp; indoor &lt; 28" sourceRef="exclusivegateway1" targetRef="scripttask7">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${TO<28 && TA<28}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask8" name="open fans A" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy open fans A");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().on();</script>
    </scriptTask>
    <sequenceFlow id="flow11" sourceRef="scripttask6" targetRef="scripttask8"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow12" sourceRef="scripttask8" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="scripttask5" targetRef="endevent1"></sequenceFlow>
    <scriptTask id="scripttask9" name="close fans A" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>out.println("SaveEnergy close fans A");
com.diplab.serializableDevice.SerializableDevice124.getInstanceOfSerializableDevice124().off();</script>
    </scriptTask>
    <sequenceFlow id="flow14" sourceRef="scripttask7" targetRef="scripttask9"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="scripttask9" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow26" sourceRef="exclusivegateway1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow27" sourceRef="scripttask1" targetRef="scripttask3"></sequenceFlow>
